library(nclSLR)
data("USArrests")
USArrestData= as.matrix(USArrests[,1:4])
dim(USArrestData)
head(USArrestData)
set.seed(1)
Kmax=10
SS_W=numeric(Kmax)
km_fit=list()
for(K in 1:Kmax)
{
km_fit[[K]] = kmeans(USArrestData, K, iter.max = 50, nstart = 20)
SS_W [K] = km_fit[[K]]$tot.withinss
}
plot(1:Kmax, SS_W,type="l", xlab="K", ylab = "SS_W")
sort(km_fit[[3]]$cluster)
km_fit[[3]]$centers
colMeans(USArrestData)
